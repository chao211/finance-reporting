{
  "name": "finance/reporting",
  "description": "Financial data reporting",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "chao",
      "email": "942633297@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "finance\\": "src/"
    }
  },
  "extra": {
    "think-provider": "finance",
    "think": {
      "services": [
        "finance\\Service"
